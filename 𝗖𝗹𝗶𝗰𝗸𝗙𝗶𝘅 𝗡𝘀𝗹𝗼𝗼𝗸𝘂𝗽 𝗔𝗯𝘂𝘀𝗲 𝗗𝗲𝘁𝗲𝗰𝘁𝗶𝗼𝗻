metadata.event_type = "PROCESS_LAUNCH"
re.regex(principal.process.command_line, `nslookup`) nocase
re.regex(principal.process.command_line, `findstr`) nocase
$ip = re.capture(principal.process.command_line, `([0-9]{1,3}\.){3}[0-9]{1,3}`)
$ip != ""
not net.ip_in_range_cidr($ip, "10.0.0.0/8")
not net.ip_in_range_cidr($ip, "172.16.0.0/12")
not net.ip_in_range_cidr($ip, "192.168.0.0/16")
