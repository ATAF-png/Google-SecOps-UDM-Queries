(metadata.event_type = "USER_LOGIN" OR metadata.event_type = "NETWORK_DNS")
AND (re.regex(principal.user.email_addresses, `\.edu$`)
OR strings.contains(principal.user.email_addresses, ".ac.")
OR strings.contains(principal.hostname, "edu")
OR strings.contains(principal.ip_geo_artifact.network.organization_name, "University")
OR strings.contains(principal.ip_geo_artifact.network.organization_name, "College")
OR strings.contains(principal.ip_geo_artifact.network.organization_name, "Education"))
AND (principal.location.country_or_region = "RU" OR principal.location.country_or_region = "CN" OR principal.location.country_or_region = "IR" OR principal.location.country_or_region = "KP" OR principal.location.country_or_region = "SY" OR principal.location.country_or_region = "CU" OR principal.location.country_or_region = "VN" OR principal.location.country_or_region = "BY" OR principal.location.country_or_region = "VE")
AND (strings.contains(target.resource.name, "admin")
OR strings.contains(target.resource.name, "login")
OR strings.contains(target.resource.name, "auth")
OR strings.contains(target.domain.name, "corp")
OR strings.contains(target.domain.name, "internal"))
AND metadata.event_timestamp.seconds > timestamp.as_unix_seconds("2024-01-01T00:00:00", "UTC")
