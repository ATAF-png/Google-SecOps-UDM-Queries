(principal.location.country_or_region = "Russia" OR principal.location.country_or_region = "China" OR principal.location.country_or_region = "North Korea" OR principal.location.country_or_region = "Iran" OR principal.location.country_or_region = "Belarus") OR (re.regex(principal.hostname, `\*chinanet\*`) OR re.regex(principal.hostname, `\*rostelecom\*`) OR re.regex(principal.hostname, `\*iranian\*`) OR re.regex(principal.hostname, `\*nkorea\*`) OR re.regex(principal.hostname, `\*mtnet\*`)) OR (principal.ip = "1.2.3.4" OR principal.ip = "5.6.7.8")
